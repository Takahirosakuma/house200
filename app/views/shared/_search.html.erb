<div class="col-md-8">
  <strong>検索</strong>
    <%= search_form_for @q ,:url=>{:controller=>"posts",:action=>"index"} do |f| %>
      <%= f.label :"タイトル" %>
      <%= f.search_field :title_cont %>
      <br>
      <%= f.label :"本文" %>
      <%= f.search_field :content_cont %>
      <br>
      <%= f.label :"都道府県" %>
      <%= f.search_field :prefecture_cont %>
      <br>
      <%= f.label :"市区町村" %>
      <%= f.search_field :city_cont %>
      <br>
    
    <%= f.submit "検索" %>
<%end%>
</div>